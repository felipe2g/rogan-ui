{
  "name": "rogan-ui",
  "version": "0.0.2",
  "description": "A component library for podcasts!",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
